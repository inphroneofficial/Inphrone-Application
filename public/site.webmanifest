{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Inphrone - People-Powered Entertainment Intelligence",
  "short_name": "Inphrone",
  "description": "World's first people-powered entertainment intelligence platform. Share opinions on Film, Music, OTT, TV, YouTube, Gaming, Social Media & App Development. Founded by Thangella Gadidamalla.",
  "start_url": "/",
  "scope": "/",
  "id": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "browser"],
  "background_color": "#1F0021",
  "theme_color": "#b91c1c",
  "orientation": "portrait-primary",
  "lang": "en",
  "dir": "ltr",
  "categories": ["entertainment", "social", "productivity", "lifestyle"],
  "iarc_rating_id": "",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "/inphrone-logo.jpg",
      "sizes": "48x48",
