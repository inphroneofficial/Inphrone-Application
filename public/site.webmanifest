      "type": "image/jpeg",
      "purpose": "any"
    },
    {
      "src": "/inphrone-logo.jpg",
      "sizes": "128x128",
      "type": "image/jpeg",
      "purpose": "any"
    },
    {
      "src": "/inphrone-logo.jpg",
      "sizes": "144x144",
      "type": "image/jpeg",
      "purpose": "any"
    },
    {
      "src": "/inphrone-logo.jpg",
      "sizes": "192x192",
