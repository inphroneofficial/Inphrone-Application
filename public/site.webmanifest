  "screenshots": [
    {
      "src": "/screenshots/inphrone-dashboard-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Inphrone Dashboard - Desktop View"
    },
    {
      "src": "/screenshots/inphrone-mobile.png",
      "sizes": "720x1280",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Inphrone Mobile Experience"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "View your activity dashboard",
      "url": "/dashboard",
      "icons": [{ "src": "/inphrone-logo.jpg", "sizes": "192x192" }]
    },
    {
      "name": "InphroSync",
      "short_name": "Sync",
      "description": "Daily entertainment pulse",
      "url": "/inphrosync",
      "icons": [{ "src": "/inphrone-logo.jpg", "sizes": "192x192" }]
    },
    {
      "name": "Insights",
      "short_name": "Insights",
      "description": "View entertainment insights",
      "url": "/insights",
      "icons": [{ "src": "/inphrone-logo.jpg", "sizes": "192x192" }]
    }
  ],
  "related_applications": [],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
